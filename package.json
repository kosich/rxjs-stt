{
  "name": "rxjs-stt",
  "version": "0.0.1",
  "description": "RxJS speech recognition wrapper",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "es2015": "./dist/es2015/index.js",
  "types": "./dist/types/index.d.ts",
  "unpkg": "./dist/rxjs-stt.min.js",
  "sideEffects": false,
  "scripts": {
    "start": "webpack --config configs/webpack.dev.js",
    "clean": "rimraf temp dist",
    "build": "npm run build:esm && npm run build:es2015 && npm run build:cjs && npm run build:types && npm run build:umd && npm run build:umd:min",
    "build:esm": "tsc -p configs/tsconfig.esm.json",
    "build:es2015": "tsc -p configs/tsconfig.es2015.json",
    "build:cjs": "tsc -p configs/tsconfig.cjs.json",
    "build:types": "tsc -p configs/tsconfig.types.json",
    "build:umd": "webpack --config configs/webpack.build.js -o dist/rxjs-stt.js",
    "build:umd:min": "webpack --config configs/webpack.build.min.js -o dist/rxjs-stt.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kosich/rxjs-stt.git"
  },
  "keywords": [
    "speech-to-text",
    "speech recognition",
    "Web Speech API",
    "s2t",
    "rxjs",
    "javascript",
    "typescript"
  ],
  "author": "Kostiantyn Palchyk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kosich/rxjs-stt/issues"
  },
  "homepage": "https://github.com/kosich/rxjs-stt#readme",
  "devDependencies": {
    "clean-webpack-plugin": "3.0.0",
    "rimraf": "3.0.2",
    "rxjs": "6.6.0",
    "ts-loader": "7.0.5",
    "typescript": "3.9.6",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-merge": "4.2.2"
  },
  "peerDependencies": {
    "rxjs": "6.6.0",
    "tslib": "2.0.0"
  },
  "files": [
    "dist"
  ]
}
